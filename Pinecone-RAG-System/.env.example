# DeepSeek (for LLM - cheaper alternative to OpenAI)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Cohere (for reranking - rerank-english-v3.0)
COHERE_API_KEY=your_cohere_api_key_here
